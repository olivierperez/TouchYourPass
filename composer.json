{
  "name": "olivierperez/touch-your-pass",
  "description": "Password manager, all encryptions are done on client side.",
  "keywords": ["password"],
  "version": "0.1",
  "license": "Apache License 2.0",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "5.5.9"
    }
  },
  "require": {
    "twbs/bootstrap": "3.3.2",
    "components/jquery": "2.1.3",
    "smarty/smarty": "3.1.43",
    "o80/i18n": "dev-develop",
    "o80/session": "dev-master",
    "ircmaxell/password-compat": "~1.0",
    "php": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5"
  },
  "autoload": {
    "psr-4": {
      "TouchYourPass\\": "app/TouchYourPass/"
    }
  }
}
